{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./public/documentation/app"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/angular-jsdoc/common/plugins/ngdoc"
  ],
  "templates": {
    "systemName": "App Name",
    "cleverLinks": true,
    "monospaceLinks": true,
    "outputSourceFiles": true,
    "linenums": true,
    "navType": "vertical",
    "dateFormat": "MMMM Do YYYY, h:mm:ss a",
    "theme": "cosmo",
    "collapseSymbols": false,
    "inverseNav": true,
    "methodHeadingReturns": false
  }
}